    <if test="_parameter!=null"><include refid="WhereClauseByEntiey_A"/></if>
    <if test="ownerId!=null">and b.ownerId=#{ownerId,jdbcType=VARCHAR}</if>
    order by a.mId, a.pid, a.sort desc
