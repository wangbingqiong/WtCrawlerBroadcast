<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- 核心配置 -->
<configuration>
  <!-- 别名设置 -->
  <typeAliases>
    <typeAlias alias="channel"      type="com.woting.crawler.core.boradcast.persis.po.ChannelPo"/>
    <typeAlias alias="programme"    type="com.woting.crawler.core.boradcast.persis.po.ProgrammePo"/>
    <typeAlias alias="region"       type="com.woting.crawler.core.boradcast.persis.po.RegionPo"/>
     <typeAlias alias="catagory"    type="com.woting.crawler.core.boradcast.persis.po.CatagoryPo"/>
  </typeAliases>
  <!-- SQL文件设置 -->
  <mappers>
    <mapper resource="com/woting/crawler/core/boradcast/persis/sql/Channel.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/boradcast/persis/sql/Programme.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/boradcast/persis/sql/Region.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/boradcast/persis/sql/Catagory.mapper.xml"/>
  </mappers>
</configuration>