<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- 核心配置 -->
<configuration>
  <!-- 别名设置 -->
  <typeAliases>
    <typeAlias alias="dictMaster"    type="com.woting.cm.core.dict.persis.po.DictMasterPo"/>
    <typeAlias alias="dictDetail"    type="com.woting.cm.core.dict.persis.po.DictDetailPo"/>
    <typeAlias alias="dictRefRes"    type="com.woting.cm.core.dict.persis.po.DictRefResPo"/>
    <typeAlias alias="broadcast"     type="com.woting.cm.core.broadcast.persis.po.BroadcastPo"/>
    <typeAlias alias="bcprogramme"   type="com.woting.cm.core.broadcast.persis.po.BCProgrammePo"/>
    <typeAlias alias="bcliveflow"    type="com.woting.cm.core.broadcast.persis.po.BCLiveFlowPo"/>
    <typeAlias alias="trackLog"      type="com.woting.cm.core.tracklog.persis.po.TrackLogPo"/>
  </typeAliases>
  <!-- SQL文件设置 -->
  <mappers>
    <mapper resource="com/woting/cm/core/dict/persis/sql/DictMaster.mapper.xml"/>
    <mapper resource="com/woting/cm/core/dict/persis/sql/DictDetail.mapper.xml"/>
    <mapper resource="com/woting/cm/core/dict/persis/sql/DictRefRes.mapper.xml"/>
    <mapper resource="com/woting/cm/core/broadcast/persis/sql/Broadcast.mapper.xml"/>
    <mapper resource="com/woting/cm/core/broadcast/persis/sql/BCProgramme.mapper.xml"/>
    <mapper resource="com/woting/cm/core/broadcast/persis/sql/BCLiveFlow.mapper.xml"/>
    <mapper resource="com/woting/cm/core/tracklog/persis/sql/TrackLog.mapper.xml"/>
  </mappers>
</configuration>